{
  "defaultRoutes": {
    "auth": "/home",
    "unauth": "/home",
    "studyPage": "/home"
  },
  "pages": [
    {
      "path": "/home",
      "pageKey": "landing",
      "rows": [
        {
          "key": "content",
          "fullWidth": false,
          "className": "py-3",
          "columns": [
            {
              "key": "videoCol",
              "className": "col-12 col-lg-8",
              "items": [
                {
                  "itemKey": "video",
                  "className": "bg-grey-7 w-100 h-100 text-white justify-content-center text-center d-flex align-items-center",
                  "config": {
                    "type": "video",
                    "posterUrlKey": "poster",
                    "fallbackTextKey": "fallbackText",
                    "videoSources": [
                      {
                        "urlKey": "mp4",
                        "type": "video/mp4"
                      },
                      {
                        "urlKey": "mxf",
                        "type": "video/mxf"
                      },
                      {
                        "urlKey": "web,",
                        "type": "video/webm"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "key": "aboutCardCol",
              "className": "col-12 col-lg-4 mt-3 mt-md-0",
              "items": [
                {
                  "itemKey": "aboutCard",
                  "className": "h-100",
                  "config": {
                    "type": "imageCard",
                    "action": {
                      "type": "navigate",
                      "value": "/about"
                    },
                    "imageSrc": "/images/about.jpeg",
                    "showActionBtn": true
                  }
                }
              ]
            },
            {
              "key": "cardResultsCol",
              "className": "col-12 col-md-6 col-lg-4 mt-3",
              "items": [
                {
                  "itemKey": "currentResultCard",
                  "className": "h-100",
                  "config": {
                    "type": "imageCard",
                    "action": {
                      "type": "navigate",
                      "value": "/results"
                    },
                    "imageSrc": "/images/results.jpg",
                    "showActionBtn": true
                  }
                }
              ]
            },
            {
              "key": "cardSignupCol",
              "className": "col-12 col-md-6 col-lg-4 mt-3",
              "items": [
                {
                  "itemKey": "signupCard",
                  "className": "h-100",
                  "config": {
                    "type": "imageCard",
                    "action": {
                      "type": "openDialog",
                      "value": "signup"
                    },
                    "imageSrc": "images/participate.jpg",
                    "showActionBtn": true
                  }
                }
              ]
            },
            {
              "key": "loginCardCol",
              "className": "col-12 col-md-12 col-lg-4 mb-3 mb-lg-0 mt-3",
              "items": [
                {
                  "itemKey": "loginCard",
                  "className": "h-100",
                  "config": {
                    "type": "loginCard",
                    "showInfoText": false
                  }
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "path": "/privacy",
      "pageKey": "privacy",
      "rows": [
        {
          "key": "content",
          "fullWidth": false,
          "className": "my-3",
          "columns": [
            {
              "key": "pCol",
              "className": "col-12 col-sm-10 col-md-8",
              "items": [
                {
                  "itemKey": "privacyPolicy",
                  "className": "",
                  "config": {
                    "type": "markdown",
                    "markdownUrl": "privacy.md"
                  }
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "path": "/faq",
      "pageKey": "faq",
