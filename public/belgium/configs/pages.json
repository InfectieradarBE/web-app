            {
              "key": "mainCol",
              "className": "col-12 col-lg-8",
              "items": [
                {
                  "itemKey": "faqTop",
                  "className": "",
                  "config": {
                    "type": "simpleCard",
                    "titleKey": "topCardTitle",
                    "contentKey": "topCardContent",
                    "variant": "h2"
                  }
                },
                {
                  "itemKey": "faq",
                  "className": "",
                  "config": {
                    "type": "accordionList",
                    "accordionCtrlsKey": "accordionControls"
                  }
                }
              ]
            },
            {
              "key": "cardSignupCol",
              "className": "col-12 col-lg-4 mt-3 mt-lg-0",
              "items": [
                {
                  "itemKey": "signupCard",
                  "className": "",
                  "config": {
                    "type": "imageCard",
                    "action": {
                      "type": "openDialog",
                      "value": "signup"
                    },
                    "imageSrc": "images/participate.jpg",
                    "showActionBtn": true
                  }
                }
              ]
            }
          ]
        }
      ]
    },
